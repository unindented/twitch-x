{
  "name": "twitch-web",
  "productName": "Twitch",
  "description": "Web client for Twitch.",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Daniel Perez Alvarez",
    "email": "unindented@gmail.com",
    "url": "https://unindented.org/"
  },
  "themeColor": "#6441A4",
  "backgroundColor": "#FFFFFF",
  "scripts": {
    "clean:build": "shx rm -rf build",
    "clean:coverage": "shx rm -rf coverage",
    "clean": "run-p clean:*",
    "test:eslint": "eslint .",
    "test:stylelint": "stylelint \"src/**/*.css\"",
    "test:jest": "jest --coverage --config config/jest/config.json",
    "test": "run-p test:*",
    "watch:jest": "jest --watch --config config/jest/config.json",
    "watch": "run-p watch:*",
    "build:dev": "webpack --config config/webpack/config.js --env development",
    "build:prod": "webpack --config config/webpack/config.js --env production",
    "build": "run-s clean build:prod",
    "start:server": "webpack-dev-server --config config/webpack/config.js --env development --hot --inline",
    "start": "run-s start:server"
  },
  "dependencies": {
    "history": "^4.7.2",
    "normalizr": "^3.2.4",
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.2.3",
    "twitch-assets": "*",
    "twitch-data": "*",
    "twitch-ui": "*"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "dotenv-safe": "^4.0.4",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.1.0",
    "shx": "^0.2.2",
    "style-ext-html-webpack-plugin": "^3.4.5",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "webmanifest-loader": "^0.2.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "workbox-webpack-plugin": "^2.1.1"
  }
}
