{
  "name": "twitch-ui",
  "description": "UI components for Twitch.",
  "version": "0.0.0",
  "license": "MIT",
  "main": "src/index.js",
  "private": true,
  "author": {
    "name": "Daniel Perez Alvarez",
    "email": "unindented@gmail.com",
    "url": "https://unindented.org/"
  },
  "scripts": {
    "clean:build": "shx rm -rf build",
    "clean:coverage": "shx rm -rf coverage",
    "clean": "run-p clean:*",
    "test:eslint": "eslint .",
    "test:stylelint": "stylelint \"src/**/*.css\"",
    "test:jest": "jest --coverage --config config/jest/config.json",
    "test": "run-p test:*",
    "watch:jest": "jest --watch --config config/jest/config.json",
    "watch": "run-p watch:*",
    "build:storybook": "build-storybook --config-dir config/storybook/ --static-dir node_modules/twitch-assets/build/ui/ --output-dir build/",
    "build": "run-s clean build:storybook",
    "start:server": "start-storybook --config-dir config/storybook/ --static-dir node_modules/twitch-assets/build/ui/ --port 8081",
    "start": "run-s start:server"
  },
  "peerDependencies": {
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "styled-components": "^2.2.0"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.2.15",
    "@storybook/addon-options": "^3.2.15",
    "@storybook/react": "^3.2.15",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "jest-styled-components": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "polished": "^1.9.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-test-renderer": "^16.1.0",
    "shx": "^0.2.2",
    "storybook-addon-rtl": "^0.1.1",
    "styled-components": "^2.2.3",
    "stylelint": "^8.2.0",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.0.0",
    "twitch-assets": "*"
  },
  "dependencies": {
    "hls.js": "^0.8.7"
  }
}
